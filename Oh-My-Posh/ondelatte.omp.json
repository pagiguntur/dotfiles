{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette":{
    "white": "#FFFFFF",
    "teal_blue": "#33658A"
  },
  "blocks": [
    // # block left
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#18354c",
          "foreground": "#ffc107",
          "leading_diamond": "\ue0b6",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "template": " \ue5ff {{ .Path }} ",
          "trailing_diamond": "\ue0b0",
          "type": "path"
        },
        {
          "background": "#18354c",
          "foreground": "#ffc107",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "type": "php",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#ffffff",
          "background": "#4063D8",
          "template": " ÓúΩ {{ .Major }}.{{.Minor}} "
        },
        {
          "background": "#303030",
          "foreground": "#3C873A",
          "leading_diamond": "\ue0b6",
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": " <#cc3a3a>\ue5fa</> ",
            "yarn_icon": " <#348cba>\ue6a7</>"
          },
          "background" : "#306998",
          "foreground" : "#FFE873",
          "style": "diamond",
          "template": "\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "trailing_diamond": "\ue0b4 ",
          "type": "node"
        }, // nodejs
               
        // rust
        {
          "type": "rust",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#193549",
          "background": "#306998",
          "template": " Óû® "
        },
        {
          "background": "#ffc107",
          "foreground": "#18354c",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "background": "#83769c",
          "foreground": "#ffffff",
          "properties": {
            "style": "roundrock",
            "threshold": 0
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }}\u2800",
          "trailing_diamond": "\ue0b4",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        
        // date time
        {
          "background": "p:teal_blue",
          "foreground": "p:white",
          "properties": {
            "time_format": "Mon 15:04"
          },
          "style": "diamond",
          "template": " \uf073 {{ .CurrentDate | date .Format }} ",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b4",
          "type": "time"
        }
      ],
      "type": "rprompt"
    }
  ],
  "console_title_template": "{{if .Root}} \u26a1 {{end}}{{.UserName}} \u2794 üìÅ{{.Folder}}",
  "final_space": true,
  "version": 3
}
